{
    "cSpell.words": [
        "adamw",
        "backpropagation",
        "checkpointed",
        "Fusi",
        "Hebb",
        "metalearners",
        "metatrain",
        "nadam",
        "radam",
        "rosenbaum",
        "serialise",
        "Unoptimized",
        "unsqeezed",
        "xaiver"
    ],
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
  },
  "isort.args":["--profile", "black"],
  "black-formatter.args": ["--line-length", "120"],
  "mypy-type-checker.args": ["--ignore-missing-imports, --no-warn-no-return"],
  "mypy-type-checker.cwd":"${workspaceFolder}/code",	
}
